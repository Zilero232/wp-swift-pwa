{
	"name": "wp-swift-pwa",
	"type": "module",
	"version": "1.0.0",
	"description": "The Progressive Web Applications (OWA) constructor with the ability to configure the manifest, service workers and other PWA application parameters.",
	"author": {
		"name": "Artemev Alexandr",
		"url": "https://github.com/Zilero232"
	},
	"license": "MIT",
	"homepage": "https://github.com/Zilero232/wp-swift-pwa#readme",
	"repository": {
		"url": "git+https://github.com/Zilero232/wp-swift-pwa.git"
	},
	"bugs": {
		"url": "https://github.com/Zilero232/wp-swift-pwa/issues"
	},
	"keywords": [
		"pwa",
		"progressive web app",
		"web app manifest",
		"service worker",
		"pwa constructor",
		"pwa builder",
		"pwa generator",
		"manifest generator",
		"web app generator",
		"offline first",
		"installable web app",
		"app shell",
		"push notifications",
		"web app optimization",
		"mobile web",
		"responsive design",
		"web capabilities",
		"web platform",
		"typescript",
		"react",
		"material-ui",
		"mui"
	],
	"config": {
		"commitizen": {
			"path": "commit-jazzer"
		}
	},
	"scripts": {
		"build": "vite build",
		"dev": "vite",
		"lint:check": "yarn eslint",
		"lint:fix": "yarn eslint --fix",
		"phpcs:check": "php ./vendor/bin/grumphp run --tasks=phpcs",
		"phpcs:fix": "php ./vendor/bin/phpcbf --standard=phpcs.xml",
		"test": "vitest run -c ./vitest.config.ts",
		"test:watch": "vitest watch -c ./vitest.config.ts",
		"commit": "yarn git-cz",
		"knip": "knip -c ./knip.json -t ./tsconfig.json",
		"cspell": "cspell lint -c ./.cspell.json --gitignore --cache --cache-location ./cache/cspellcache",
		"prepare": "husky"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@mui/icons-material": "^6.2.0",
		"@mui/material": "^6.2.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"zustand": "^5.0.2"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^3.8.0",
		"@cspell/dict-ru_ru": "^2.2.4",
		"@eslint/js": "^9.15.0",
		"@kucrut/vite-for-wp": "^0.9.4",
		"@types/node": "^22.9.0",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/coverage-v8": "^2.1.5",
		"@vitest/ui": "^2.1.5",
		"commander": "^12.1.0",
		"commit-jazzer": "^1.0.5",
		"cspell": "^8.16.0",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-format": "^0.1.2",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.37.2",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.16",
		"globals": "^15.12.0",
		"husky": "^9.1.6",
		"knip": "^5.36.3",
		"lint-staged": "^15.2.10",
		"rollup-plugin-external-globals": "^0.13.0",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.15.0",
		"vite": "^6.0.1",
		"vite-plugin-external": "^4.3.1",
		"vite-tsconfig-paths": "^5.1.0",
		"vitest": "^2.1.4"
	}
}
